.ch-navbar-custom {
	display: block;
	position: relative;
}
.ch-navbar {
	display: flex;
	position: relative;
	align-items: center;
	justify-content: space-between;
	padding-right: 220rpx;
}
.ch-navbar.navbar-light {
	color: #050607;
	background-color: #fff;
}
.ch-navbar.navbar-black {
	color: #fff;
	background-color: #050607;
}
.ch-navbar.navbar-grad {
	background-image: linear-gradient(rgba(0, 0, 0, 0.3), rgba(0, 0, 0, 0));
}
.ch-navbar .ch-navbar-action {
	display: flex;
	align-items: center;
	height: 100%;
	justify-content: center;
	max-width: 100%;
}
.ch-navbar .ch-navbar-action .icon-back {
  width: 44rpx;
  height: 44rpx;
  margin-right: 40rpx;
	background-image:url('data:image/svg+xml;base64,');
	background-repeat: no-repeat;
	background-size: cover;
}
.ch-navbar.navbar-black .ch-navbar-action .icon-back {
	background-image:url('data:image/svg+xml;base64,');
}
.ch-navbar .ch-navbar-action image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 40rpx;
}
.ch-navbar .ch-navbar-action:first-child {
	margin-left: 20rpx;
	font-size: 30rpx;
}

.ch-navbar .ch-navbar-action:last-child {
	margin-right: 30rpx;
}

.ch-navbar .ch-navbar-content {
	position: absolute;
	text-align: center;
	width: calc(100% - 440rpx);
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	height: 60rpx;
	font-size: 32rpx;
	line-height: 60rpx;
	cursor: none;
	pointer-events: none;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}

.ch-navbar.ios .ch-navbar-content {
	bottom: 7px;
	height: 30px;
	font-size: 32rpx;
	line-height: 30px;
}

.ch-navbar.fixed {
	position: fixed;
	width: 100%;
	top: 0;
	z-index: 98;
}

.ch-navbar .ch-navbar-tab {
	display: flex;
	align-items: center;
	position: absolute;
	text-align: center;
	width: 256rpx;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	height: 56rpx;
	line-height: 56rpx;
	border-radius: 56rpx;
	background-image: linear-gradient(90deg, #FFEF2A 0%, #FFD100 100%);
}

.ch-navbar .ch-navbar-tab .ch-navbar-tab-item {
	flex: 1;
	font-size: 28rpx;
	color: #484644;
	z-index: 1;
}

.ch-navbar .ch-navbar-tab .ch-navbar-tab-item.ch-navbar-tab-actived {
	color: #000;
}

.ch-navbar .ch-navbar-tab .ch-navbar-tab-indicator {
  position: absolute;
  top: 4rpx;
  left: 4rpx;
  width: 120rpx;
  height: 48rpx;
  border-radius: 48rpx;
  background-color: #fff;
  transition: transform 0.3s;
	z-index: 0;
}
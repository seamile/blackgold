page {
    background: #f7f7f7;
}

.main-wrap {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: 8%;
}

.main-wrap>image {
    margin: 0 auto;
    width: 558rpx;
    height: 920rpx;
}

.main-wrap .money-box {
    position: absolute;
    left: 0;
    top: 195rpx;
    width: 100%;
    text-align: center;
    color: #ffe696;
}

.main-wrap .money-box .num {
    line-height: 168rpx;
    font-size: 120rpx;
}

.main-wrap .money-box .unit {
    font-size: 54rpx;
}

.main-wrap .money-box .tip {
    display: block;
    line-height: 62rpx;
    font-size: 44rpx;
}

.main-wrap>button {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 160rpx;
    width: 444rpx;
    height: 114rpx;
}

.main-wrap .icon {
    position: absolute;
    bottom: 31rpx;
    left: 258rpx;
    width: 40rpx;
    height: 30rpx;
}

.share-tip {
    position: absolute;
    width: 230rpx;
    height: 64rpx;
    bottom: 280rpx;
    left: 164rpx;
    line-height: 51rpx;
    padding-left: 21rpx;
    box-sizing: border-box;
    font-size: 26rpx;
    color: #e22915;
    background-image: url(data:image/png;base64,);
    background-size: 100% auto;
    background-position: 0 0;
}
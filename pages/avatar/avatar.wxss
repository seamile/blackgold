.ch-add-tips {
  position: fixed;
  top: 150rpx;
  right: 50rpx;
  padding: 10rpx 70rpx 10rpx 30rpx;
  border-radius: 100rpx;
  color: #000;
  font-size: 13px;
  text-align: left;
  z-index: 999;
  background-image: linear-gradient(90deg,#ffef2a,#ffd100);
  box-shadow: 0 8rpx 16rpx 0 rgba(245,166,35,.2);
  animation: bounce .6s cubic-bezier(.58,.1,.58,.25) infinite alternate;
}

.ch-add-tips:before {
  top: -10rpx;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  border-bottom: 15rpx solid #feda36;
  z-index: 2001;
}

.ch-add-tips:after,.ch-add-tips:before {
  content: "";
  position: absolute;
  right: 100rpx;
  width: 0;
  height: 0;
  transform: translateX(50%);
}

.ch-add-tips:after {
  top: -14rpx;
  border-left: 16rpx solid transparent;
  border-right: 16rpx solid transparent;
  border-bottom: 16rpx solid #feda36;
}

.ch-add-tips .ch-close-tips {
  position: absolute;
  right: 20rpx;
  top: 50%;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  background-color: #ffef2a;
  transform: translateY(-50%);
}

.ch-add-tips .ch-close-tips:after,.ch-add-tips .ch-close-tips:before {
  content: "";
  position: absolute;
  left: 50%;
  top: 50%;
  width: 16rpx;
  height: 4rpx;
  border-radius: 4rpx;
  background-color: rgba(0,0,0,.4);
}

.ch-add-tips .ch-close-tips:before {
  transform: translate(-50%,-50%) rotate(45deg);
}

.ch-add-tips .ch-close-tips:after {
  transform: translate(-50%,-50%) rotate(-45deg);
}


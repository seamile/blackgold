<!--基础元素-->
<template name="wxParseVideo">
  <!--增加video标签支持，并循环添加-->
  <view class="{{item.classStr}} wxParse-{{item.tag}}" style="{{item.styleStr}}">
    <video object-fit="contain" class="{{item.classStr}} wxParse-{{item.tag}}-video" src="{{item.attr.src}}" poster='{{item.attr.poster}}'></video>
  </view>
</template>
<template name="wxParseAudio">
  <!--增加audio标签支持，并循环添加-->
  <view class="{{item.classStr}} wxParse-{{item.tag}}" style="{{item.styleStr}}">
    <view class="wxParseAudioView" data-poster="{{item.attr.poster}}" data-title="{{item.attr.title}}" data-author="{{item.attr.author}}" data-src="{{item.attr.src}}" bindtap="wxParseAudioPlay">
      <view class="wxParseAudioButton">
        <image class="AudioPlayImg" src="../../image/play_icon_show.png"></image>
      </view>
      <view class="AudioTitleText">
        <text>{{item.attr.title ? item.attr.title : '未知作品辑'}}{{item.attr.author ? ' - ' + item.attr.author : ''}}</text>
      </view>
    </view>
  </view>
</template>
<template name="wxParseImg">
  <imageload class="{{item.classStr}} wxParse-{{item.tag}}" data-from="{{item.from}}" data-src="{{item.attr.src}}" data-idx="{{item.imgIndex}}" src="{{item.attr.src}}" mode="aspectFit" bindload="wxParseImgLoad" bindtap="wxParseImgTap" mode="widthFix" style="width:{{item.width}}px;"
  />
</template>
<!--33行加入text组件，用于复制文字-->
<template name="WxEmojiView">
  <view class="WxEmojiView wxParse-inline" style="{{item.styleStr}}">
    <block wx:for="{{item.textArray}}" wx:key="item">
      <block class="{{item.text == '\\n' ? 'wxParse-hide':''}}" wx:if="{{item.node == 'text'}}"><text selectable="{{true}}">{{item.text}}</text></block>
      <block wx:elif="{{item.node == 'element'}}">
        <image class="wxEmoji" src="{{item.baseSrc}}{{item.text}}" />
      </block>
    </block>
  </view>
</template>
<!--丸子小程序商品信息,请保留此信息 By Chi-->
<template name="wxParseMiniProgram">
  <view class="wxParse-Card-View" data-appid="{{item.attr.appid}}" data-path="{{item.attr.path}}" bindtap="wxParseToMiniProgram">
    <view class="wxParse-Card-box">
      <view class="wxParse-Card-thumbnail">
        <image mode="aspectFill" src="{{item.attr.thumb}}" />
      </view>
      <view class="wxParse-Card-content">
        <view class="wxParse-Card-title-box">
          <view class="wxParse-Card-title">{{item.attr.title}}</view>
        </view>
        <view class="wxParse-Card-text-box">
          <view class="wxParse-Card-text-red">{{item.attr.price?'￥'+item.attr.price:''}}</view>
          <button class="wxParse-Card-button wxParse-Card-red">{{item.attr.button}}</button>
        </view>
      </view>
    </view>      
  </view>
</template>
<!--丸子小程序公众号,请保留此信息 By Chi-->
<template name="wxParseMpAccounts">
  <view class="wxParse-Card-View" data-url="{{item.attr.url}}" bindtap="wxParseToOfficialAccounts">
    <view class="wxParse-Card-box">
      <view class="wxParse-Card-thumbnail">
        <image mode="aspectFill" src="{{item.attr.thumb}}"/>
      </view>
      <view class="wxParse-Card-content">
        <view class="wxParse-Card-title-box">
          <view class="wxParse-Accounts-title">{{item.attr.title}}</view>
          <view class="wxParse-Card-description">{{item.attr.description}}</view>
        </view>
        <view class="wxParse-Card-text-box">
          <view class="wxParse-Card-text-grey">{{item.attr.text}}</view>
          <button class="wxParse-Card-button wxParse-Card-green">{{item.attr.button}}</button>
        </view>
      </view>
    </view>       
  </view>
</template>
<!--丸子小程序文章卡片,请保留此信息 By Chi-->
<template name="wxParseMpCard">
  <navigator class="wxParse-Card-View" url="{{item.attr.path}}" hover-class="navigator-hover">
    <view class="wxParse-Card-box">
      <view class="wxParse-Card-thumbnail">
        <image mode="aspectFill" src="{{item.attr.thumb}}"/>
      </view>
      <view class="wxParse-Card-content">
        <view class="wxParse-Card-title-box">
          <view class="wxParse-Card-title">{{item.attr.title}}</view>
        </view>
        <view class="wxParse-Card-text-box">
          <view class="wxParse-Card-text-grey"></view>
          <button class="wxParse-Card-button wxParse-Card-green">{{item.attr.button}}</button>
        </view>
      </view>
    </view>       
  </navigator>
</template>
<!--丸子小程序文章订阅,请保留此信息 By Chi-->
<template name="wxParseSubscribe">
  <view class="WxEmojiView wxParseSubscribe">
    <button class="wxParse-Card-button wxParse-Card-green" data-credit="{{item.attr.credit}}" open-type="getUserInfo" bindgetuserinfo="creditRead">使用 {{item.attr.credit}} 积分查看完整内容</button>
  </view>
</template>
<!--丸子小程序文档下载,请保留此信息 By Chi-->
<template name="wxParseDocument">
  <view class="wxParse-Card-View">
    <view class="wxParse-Card-box">
      <view class="wxParse-Card-thumbnail">
        <image mode="aspectFill" src="{{item.attr.thumb}}" />
      </view>
      <view class="wxParse-Card-content">
        <view class="wxParse-Card-title-box">
          <view class="wxParse-Accounts-title">{{item.attr.title}}</view>
          <view class="wxParse-Card-description">{{item.attr.description}}</view>
        </view>
        <view class="wxParse-Card-text-box">
          <view wx:if="{{item.attr.url}}" class="wxParse-Card-text-grey">{{item.attr.document?'文档: '+item.attr.document:''}}</view>
          <view wx:else class="wxParse-Card-text-grey">{{item.attr.credit?'积分: '+item.attr.credit:''}}</view>
          <button wx:if="{{item.attr.url}}" class="wxParse-Card-button wxParse-Card-green"  data-url="{{item.attr.url}}" bindtap="wxParseOpenDocument">查看文档</button>
          <button wx:else class="wxParse-Card-button wxParse-Card-red"  data-credit="{{item.attr.credit}}" open-type="getUserInfo" bindgetuserinfo="creditRead">立即购买</button>
        </view>
      </view>
    </view>       
  </view>
</template>
<!--丸子小程序商城产品信息,请保留此信息 By Chi-->
<template name="wxParseMPStore">
  <view class="wxParse-Card-View" id="{{item.attr.id}}" bindtap="wxParseProductDetail">
    <view class="wxParse-Card-box">
      <view class="wxParse-Card-thumbnail">
        <image mode="aspectFill" src="{{item.attr.thumb}}" />
      </view>
      <view class="wxParse-Card-content">
        <view class="wxParse-Card-title-box">
          <view class="wxParse-Card-title">{{item.attr.title}}</view>
        </view>
        <view class="wxParse-Card-text-box">
          <view wx:if="{{item.attr.kinds == 'normal'}}" class="wxParse-Card-text-red">{{item.attr.price?'￥'+item.attr.price:''}}</view>
          <view wx:else class="wxParse-Card-text-red wxParse-Card-credit">{{item.attr.credit?'积分: '+item.attr.credit:''}}</view>
          <button class="wxParse-Card-button wxParse-Card-red">查看</button>
        </view>
      </view>
    </view>
  </view>
</template>
<template name="WxParseBr">
  <text>\n</text>
</template>
<!--入口模版-->

<template name="wxParse">
  <block wx:for="{{wxParseData}}" wx:key="item">
    <template is="wxParse0" data="{{item}}" />
  </block>
</template>


<!--循环模版-->
<template name="wxParse0">
  <!--<template is="wxParse1" data="{{item}}" />-->
  <!--判断是否是标签节点-->
  <block wx:if="{{item.node == 'element'}}">
    <block wx:if="{{item.tag == 'button'}}">
      <button type="default" size="mini">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="item">
          <template is="wxParse1" data="{{item}}" />
        </block>
      </button>
    </block>
    <!--li类型-->
    <block wx:elif="{{item.tag == 'li'}}">
      <view class="{{item.classStr}} wxParse-li" style="{{item.styleStr}}">
        <view class="{{item.classStr}} wxParse-li-inner">
          <view class="{{item.classStr}} wxParse-li-text">
            <view class="{{item.classStr}} wxParse-li-circle"></view>
          </view>
          <view class="{{item.classStr}} wxParse-li-text">
            <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="item">
              <template is="wxParse1" data="{{item}}" />
            </block>
          </view>
        </view>
      </view>
    </block>

    <!--video类型-->
    <block wx:elif="{{item.tag == 'video'}}">
      <template is="wxParseVideo" data="{{item}}" />
    </block>

    <!--audio类型-->
    <block wx:elif="{{item.tag == 'audio'}}">
      <template is="wxParseAudio" data="{{item}}" />
    </block>

    <!--img类型-->
    <block wx:elif="{{item.tag == 'img'}}">
      <template is="wxParseImg" data="{{item}}" />
    </block>

    <!--a类型-->
    <block wx:elif="{{item.tag == 'a'}}">
      <view bindtap="wxParseTagATap" class="wxParse-inline {{item.classStr}} wxParse-{{item.tag}}" data-appid="{{item.attr.appid}}" data-type="{{item.attr.type}}" data-url="{{item.attr.path}}" data-src="{{item.attr.href}}" style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="item">
          <template is="wxParse1" data="{{item}}" />
        </block>
      </view>
    </block>
    <block wx:elif="{{item.tag == 'table'}}">
      <view class="{{item.classStr}} wxParse-{{item.tag}}" style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="item">
          <template is="wxParse1" data="{{item}}" />
        </block>
      </view>
    </block>

    <block wx:elif="{{item.tag == 'br'}}">
      <template is="WxParseBr"></template>
    </block>
    <block wx:elif="{{item.tag == 'miniprogram'}}">
      <template is="wxParseMiniProgram" data="{{item}}" />
    </block>
    <block wx:elif="{{item.tag == 'official-accounts'}}">
      <template is="wxParseMpAccounts" data="{{item}}" />
    </block>
    <block wx:elif="{{item.tag == 'mp_card'}}">
      <template is="wxParseMpCard" data="{{item}}" />
    </block>
    <block wx:elif="{{item.tag == 'subscribe'}}">
      <template is="wxParseSubscribe" data="{{item}}" />
    </block>
    <block wx:elif="{{item.tag == 'document'}}">
      <template is="wxParseDocument" data="{{item}}" />
    </block>
    <block wx:elif="{{item.tag == 'mp_store'}}">
      <template is="wxParseMPStore" data="{{item}}" />
    </block>
    <!--其他块级标签-->
    <block wx:elif="{{item.tagType == 'block'}}">
      <view class="{{item.classStr}} wxParse-{{item.tag}}" style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="item">
          <template is="wxParse1" data="{{item}}" />
        </block>
      </view>
    </block>

    <!--内联标签-->
    <view wx:else class="{{item.classStr}} wxParse-{{item.tag}} wxParse-{{item.tagType}}" style="{{item.styleStr}}">
      <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="item">
        <template is="wxParse1" data="{{item}}" />
      </block>
    </view>

  </block>

  <!--判断是否是文本节点-->
  <block wx:elif="{{item.node == 'text'}}">
    <!--如果是，直接进行-->
    <template is="WxEmojiView" data="{{item}}" />
  </block>

</template>



